{
  "name": "eslint-plugin-grouped-css-declarations",
  "version": "0.11.0",
  "description": "",
  "main": "./lib/index.js",
  "types": "./src/index.ts",
  "files": [
    "lib/**",
    "src/**"
  ],
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "css",
    "linaria",
    "grouping",
    "declarations"
  ],
  "scripts": {
    "build": "rm -rf ./lib && tsc --project ./tsconfig.build.json",
    "watch": "yarn build --watch",
    "eslint:files": "eslint",
    "eslint:plugin": "yarn eslint:files 'src/**/*.{ts,tsx}' --ignore-pattern '**/tests/**/*.ts' --ignore-pattern '**/tests/**/*.tsx'",
    "eslint:tests": "yarn eslint:files 'src/**/tests/**/*.{ts,tsx}'",
    "eslint:dummy": "yarn eslint:files 'dummy/**/debug.{json,ts,tsx}'",
    "stylelint:files": "stylelint",
    "stylelint:dummy": "yarn stylelint './dummy/**/*.css'",
    "prepublishOnly": "yarn build",
    "test": "jest --cache --config=.jestrc.json",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@linaria/core": "^4.2.10",
    "@swc-node/jest": "^1.6.7",
    "@swc/core": "^1.3.78",
    "@swc/helpers": "^0.5.1",
    "@types/css": "^0.0.33",
    "@types/eslint": "^8.44.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/scope-manager": "^6.4.0",
    "csstype": "^3.1.2",
    "enhanced-resolve": "^5.15.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-comment-length": "^1.6.1",
    "eslint-plugin-grouped-css-declarations": "link:./",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.3",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.4.0",
    "postcss": "^8.4.28"
  }
}
