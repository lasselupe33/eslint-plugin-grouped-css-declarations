{
  "name": "eslint-plugin-grouped-css-declarations",
  "version": "0.8.1",
  "description": "",
  "main": "./lib/index.js",
  "files": [
    "lib/**"
  ],
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "css",
    "linaria",
    "grouping",
    "declarations"
  ],
  "scripts": {
    "build": "rm -rf ./lib && tsc --project ./tsconfig.build.json",
    "watch": "yarn build --watch",
    "eslint:files": "eslint",
    "eslint:plugin": "yarn eslint:files 'src/**/*.{ts,tsx}' --ignore-pattern '**/tests/**/*.ts' --ignore-pattern '**/tests/**/*.tsx'",
    "eslint:tests": "yarn eslint:files 'src/**/tests/**/*.{ts,tsx}'",
    "eslint:dummy": "yarn eslint:files 'dummy/**/debug.{json,ts,tsx}'",
    "stylelint:files": "stylelint",
    "stylelint:dummy": "yarn stylelint './dummy/**/*.css'",
    "prepublishOnly": "yarn build",
    "test": "jest --cache --config=.jestrc.json",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@linaria/core": "^4.2.10",
    "@swc-node/jest": "^1.6.5",
    "@swc/core": "^1.3.62",
    "@types/css": "^0.0.33",
    "@types/eslint": "^8.40.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@typescript-eslint/scope-manager": "^5.59.8",
    "csstype": "^3.1.2",
    "enhanced-resolve": "^5.14.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-comment-length": "^1.1.0",
    "eslint-plugin-grouped-css-declarations": "link:./",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.3",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.59.8",
    "postcss": "^8.4.24"
  }
}
