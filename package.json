{
  "name": "eslint-plugin-grouped-css-declarations",
  "version": "0.2.2",
  "description": "",
  "main": "./lib/index.js",
  "files": [
    "lib/**"
  ],
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "css",
    "linaria",
    "grouping",
    "declarations"
  ],
  "scripts": {
    "build": "rm -rf ./lib && tsc --project ./tsconfig.build.json",
    "watch": "yarn build --watch",
    "eslint:files": "eslint",
    "eslint:plugin": "yarn eslint:files 'src/**/*.{ts,tsx}' --ignore-pattern '**/tests/**/*.ts' --ignore-pattern '**/tests/**/*.tsx'",
    "eslint:tests": "yarn eslint:files 'src/**/tests/**/*.{ts,tsx}'",
    "eslint:dummy": "yarn eslint:files 'dummy/**/debug.{json,ts,tsx}'",
    "prepublishOnly": "yarn build",
    "test": "jest --cache --config=.jestrc.json",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@linaria/core": "^4.2.8",
    "@swc-node/jest": "^1.6.2",
    "@swc/core": "^1.3.38",
    "@types/css": "^0.0.33",
    "@types/eslint": "^8.21.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@typescript-eslint/scope-manager": "^5.54.1",
    "csstype": "^3.1.1",
    "enhanced-resolve": "^5.12.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-comment-length": "^0.9.2",
    "eslint-plugin-grouped-css-declarations": "link:./",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.54.1",
    "postcss": "^8.4.21"
  }
}
